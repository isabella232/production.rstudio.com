baseurl = "/"
languageCode = "en-us"
title = "R in Production"
theme = "rstudio-docs-theme"
googleAnalytics = ""
ignoreFiles = ["\\.Rmd$", "\\.Rmarkdown$", "_files$", "_cache$"]
canonifyurls = true
pluralizeListTitles = "false"

[params]
    author = "RStudio"
    description = "Resources for using R in production."
    repo_url = "github.com/rstudio/production.rstudio.com"
    
    logo = "/rstudio-logo.png"
    favicon = "favicon.png" 

    # options for highlight.js (version, additional languages, and theme)
    highlightjsVersion = "9.11.0"
    highlightjsCDN = "//cdn.bootcss.com"
    highlightjsLang = ["r", "yaml"]
    highlightjsTheme = "github"

    MathJaxCDN = "//cdn.bootcss.com"
    MathJaxVersion = "2.7.1"
    
[params.palette]
    primary = "rstudio-dark"
    accent = "orange"
    
[params.font]
    text = "sans-serif"
    code = "Ubuntu Mono"

[[menu.main]]
    weight = 1
    name = "About"
    url = "/about/"
    
[[menu.main]]
    weight = 2
    name = "Security"
    url = "about/security/"
    parent = "About"

[social]
    github = "rstudio/production.rstudio.com"



